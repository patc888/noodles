#!/bin/bash

for i in {0..5}
do
  curl -s "http://itembrowser.com/rarebee.php?page=$i&view=" | perl -ne 'if(m/Detail">(\d+)\. (.+)<\/a>/){ print "$1|$2\n"; }'
done

